@inherits Umbraco.Web.Mvc.UmbracoViewPage
@using ClientDependency.Core.Mvc
@{
	Layout = null;

	
    }


    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title></title>

        @{
        Html.RequiresCss("https://use.fontawesome.com/releases/v5.8.2/css/all.css");
        Html.RequiresCss("https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/css/bootstrap.min.css");
        Html.RequiresCss("https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/css/mdb.min.css");
        Html.RequiresCss("https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap");
        Html.RequiresCss("/css/style.css");
        Html.RequiresCss("/css/RTE.css");

        Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js");
        Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js");
        Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/js/bootstrap.min.js");
        Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/js/mdb.min.js");
        Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js");
        Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js");

        }

        @Html.RenderCssHere()




    </head>

    <body>

        <!--navbar -->
        @Html.Partial("Navbar")
        @Html.Partial("Hero")


        @RenderBody()

        <!-- footer -->
        @Html.Partial("Footer")

        @Html.RenderJsHere()
    </body>
</html>

